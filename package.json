{
  "name": "typings-env-node",
  "version": "1.0.0",
  "description": "Typescript typings for nodejs",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p 0.10 && tsc -p 0.12 && tsc -p 4.0 && tsc -p 6.0",
    "build-es6": "tsc --target es6 -p 0.10 && tsc --target es6 -p 0.12 && tsc --target es6 -p 4.0 && tsc --target es6 -p 6.0",
    "test": "tsc -p 0.10/test && tsc -p 0.12/test && tsc -p 4.0/test && tsc -p 6.0/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typed-typings/env-node.git"
  },
  "author": "Louay Alakkad <louay@alakkad.me>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/typed-typings/env-node/issues"
  },
  "homepage": "https://github.com/typed-typings/env-node#readme",
  "devDependencies": {
    "typescript": "^2.0.3"
  }
}
